local Tab1 = Window:NewTab("Item")
local Section1 = Tab1:NewSection("Custom Crucifix")
local Section2 = Tab1:NewSection("Item")

Section1:NewButton("Figure Crucifix", "ButtonInfo", function()
loadstring(game:HttpGet('https://raw.githubusercontent.com/huyhoangphuc/hgh/main/Ghggg'))()
